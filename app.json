{
  "expo": {
    "name": "DeepTime",
    "slug": "DeepTime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/DeepTimeIcon.png",
    "splash": {
      "image": "./assets/DeepTimeSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f4c82"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/DeepTimeNoti.png"
    },
    "android": {
      "package": "net.danielwind.pictionary",
      "icon": "./assets/DeepTimeNoti.png"
    },
    "notification": {
      "icon": "./assets/DeepTimeNoti.png",
      "color": "#002C56",
      "iosDisplayInForeground": true,
      "androidMode": "default"
    }
  }
}
